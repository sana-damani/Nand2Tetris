/** 
* IsNeg16:
 * out = 0 if in[16] < 0
 *       1 otherwise
 */

 CHIP IsNeg16 {
    IN in[16];
    OUT out;

    PARTS:
    // Put your code here:
    Or8Way(in=in[8..15], out=out);
}
