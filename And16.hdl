/**
 * 16-bit bitwise And:
 * for i = 0..15: out[i] = (a[i] and b[i])
 */

CHIP And16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
    // Put your code here:
    // a AND b = NOT(NOT(a) or NOT(b))
    Not16(in=a, out=nota);
    Not16(in=b, out=notb);
    Or16(a=nota, b=notb, out=temp);
    Not16(in=temp, out=out); 
}
